<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <managed-bean>
        <description>Control del Menu</description>
        <managed-bean-name>menuView</managed-bean-name>
        <managed-bean-class>vista.MenuView</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <description>Visa de Login para Usuario</description>
        <managed-bean-name>userLoginView</managed-bean-name>
        <managed-bean-class>vista.UserLoginView</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
